{
  "name": "@sometimesicode/retrickable-ts",
  "version": "0.0.1",
  "private": false,
  "description": "A Typescript wrapper for the Rebrickable Lego API",
  "keywords": [
    "rebrickable",
    "Rebrickable",
    "rebrickable-ts",
    "Rebrickable-ts",
    "Lego"
  ],
  "bugs": "https://github.com/kevanstuart/rebrickable-ts/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevanstuart/rebrickable-ts"
  },
  "license": "MIT",
  "author": {
    "name": "Kevan Stuart",
    "url": "http://sometimesicode.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsup src/index.ts --format cjs,esm --dts",
